@{
    ViewData["Title"] = "Location of Users";
}
<div class="row">
    <div class="col">
        Location of all users
        <div id="map" style="height:500px; width:1000px"></div>
        <script>
            var map;
            function initMap(){
                map = new google.maps.Map(document.getElementById('map'),{
                    center: {lat: 33.9698, lng: 25.4937 },
                    zoom: 8
                });
            }
        </script>
        <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDOkL7eEkfIR3hoVijVURp4PzXgSRFWiaM&callback=initMap" async defer> //change api key
            </script>
    </div>
</div>
